{
    "name": "karmadice/laravel-log-enhacer",
    "description": "Make debugging easier by adding more data to your laravel logs",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.6.*|3.7.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Krutarth Patel",
            "email": "krutarth.patel7@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "karmadice\\LaravelLogEnhancer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "karmadice\\LaravelLogEnhancer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "karmadice\\LaravelLogEnhancer\\Providers\\LaravelLogEnhancerServiceProvider"
            ]
        }
    }
}
